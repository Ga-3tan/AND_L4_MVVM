<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/main_note_list_fragment_tablet_landscape"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:name="heig.and.lab4.NoteListFragment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/main_tablet_menu_fragment_landscape"
        app:layout_constraintHorizontal_weight="2"/>

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/main_tablet_menu_fragment_landscape"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:name="heig.and.lab4.TabletMenuFragment"
        app:layout_constraintStart_toEndOf="@+id/main_note_list_fragment_tablet_landscape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_weight="1"/>

</androidx.constraintlayout.widget.ConstraintLayout>